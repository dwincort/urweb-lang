{
  "name": "urweb-lang",
  "displayName": "urweb-language",
  "description": "Syntax highlighting and code navigation for Ur/Web",
  "version": "0.0.4",
  "license": "MIT",
  "publisher": "DanWC",
  "engines": {
    "vscode": "^1.83.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dwincort/urweb-lang.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:ur",
    "onLanguage:urs"
  ],
  "contributes": {
    "languages": [{
      "id": "ur",
      "aliases": ["UrWeb", "ur"],
      "extensions": [".ur"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/ur.png",
        "dark": "./icons/ur.png"
      }
    },{
      "id": "urs",
      "aliases": ["UrWebSignature", "urs"],
      "extensions": [".urs"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/urs.png",
        "dark": "./icons/urs.png"
      }
    },{
      "id": "urp",
      "aliases": ["UrWebProject", "urp"],
      "extensions": [".urp"],
      "configuration": "./language-configuration-urp.json",
      "icon": {
        "light": "./icons/urp.png",
        "dark": "./icons/urp.png"
      }
    }],
    "grammars": [{
      "language": "ur",
      "scopeName": "source.ur",
      "path": "./syntaxes/urweb.tmLanguage"
    },{
      "language": "urs",
      "scopeName": "source.ur",
      "path": "./syntaxes/urweb.tmLanguage"
    },{
      "language": "urp",
      "scopeName": "source.urp",
      "path": "./syntaxes/urwebproj.tmLanguage"
    }],
    "snippets": [{
      "language": "ur",
      "path": "./snippets/urweb-snippets.json"
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.83.0",
    "typescript": "^5.0.0"
  }
}
